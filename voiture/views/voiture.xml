<odoo>
  <data>
    <record id="voiture_form_view" model="ir.ui.view" >
      <field name="name">Voiture Form</field>
      <field name="model">voiture</field>
      <field name="arch" type="xml">
        <form>
          <header>
            <button string = "CHECK" type ="object" name = "is_dispo"/>
          </header>
          <sheet>
            <group>
              <field name = "modèle"/>
              <field name = "série" />
              <field name = "matricule"/>
              <field name = "dis"/>
              <field name = "image" widget="image"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <record id="voiture_tree_view" model="ir.ui.view">
      <field name="name">Voiture List</field>
      <field name="model">voiture</field>
      <field name="arch" type="xml">
        <tree>
          <field name = "modèle"/>
          <field name = "série" />
          <field name = "matricule"/>
          <field name = "dis"/>
          <field name = "image"/>
        </tree>
      </field>
    </record>
   
    

